@using DigitalBreakthrough.ViewModels
@using DigitalBreakthrough.Enums
@model List<AppointmentModel>
@foreach (AppointmentModel item in Model)
{
    @item.Id
    <br />
    @item.DoctorId
    <br />
    @item.DoctorName
    <br />
    @item.Time
    <br />
    @item.Status
    <br />
    @EventStatus.Awaiting
    @EventStatus.Canceled
    @EventStatus.Finished
    @EventStatus.Forming
    @EventStatus.InProgress
    <br />
    @(item.Status == EventStatus.Finished ? "test" : "asd")
    <br />
}
